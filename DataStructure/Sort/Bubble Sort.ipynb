   "source": [
    "## Bubble Sort\n",
    "def Bubble(lst):\n",
    "    \n",
    "    for i in range(len(lst) - 1):\n",
    "        for j in range(len(lst) - i - 1):\n",
    "            if lst[j] > lst[j + 1]:\n",
    "                lst[j], lst[j + 1] = lst[j + 1], lst[j]\n",
    "    return lst\n",
    "## Test\n",
    "lst = [3,4,2,5,6,1]\n",
    "print(Bubble(lst))"
   ]
  }
